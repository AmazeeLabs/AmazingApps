{#graphql
query ($node: String!) {
  node:nodeById(id: $node) {
    type:__typename
    ...Page
    ...Article
    ...TeamMember
    ...Job
  }
}
#}
{% if graphql.node.type == 'NodePage'%}
  {% include '#page' with graphql.node %}
{% endif %}

{% if graphql.node.type == 'NodeArticle'%}
  {% include '#article' with graphql.node %}
{% endif %}

{% if graphql.node.type == 'NodeTeamMember'%}
  {% include '#team-member' with graphql.node %}
{% endif %}

{% if graphql.node.type == 'NodeJobOffer'%}
  {% include '#job' with graphql.node %}
{% endif %}
